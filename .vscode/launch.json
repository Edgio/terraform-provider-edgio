{
  "version": "0.2.0",
  "configurations": [
    {
        "name": "Debug Go Tests",
        "type": "go",
        "request": "launch",
        "mode": "test",
        "program": "${workspaceFolder}/internal/edgio_provider/resources",
        "args": [
            "-test.run", "^TestPropertyResource_Create$"
